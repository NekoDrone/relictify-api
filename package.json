{
  "name": "relictify-api",
  "version": "1.0.0",
  "description": "Backend API service for Relictify.",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc && babel src -d build --extensions '.ts'",
    "start": "npm ci --omit=dev && PROD_ENV=prod node build/app.js",
